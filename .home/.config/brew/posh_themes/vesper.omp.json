{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#a0a0a0",
          "style": "plain",
          "template": "",
          "alignment": "top",
          "type": "text"
        },
        {
          "foreground": "#a0a0a0",
          "properties": {
            "style": "folder"
          },
          "style": "plain",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "#ffc799",
          "properties": {
            "branch_icon": ""
          },
          "style": "plain",
          "template": "<#ffc799>(</>{{ .HEAD }}<#ffc799>)</>",
          "type": "git"
        },
        {
          "foreground": "#ebbcba",
          "style": "plain",
          "template": " \u2717",
          "type": "status"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 3
}
